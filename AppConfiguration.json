{
    "MyAppName": "IceStore",
    "MyAppVersion": "1.0",
    "MyAppPublisher": "ElmlaHSoft",
    "MyAppURL": "",
    "MyAppIcon": "iceStore.ico",
    "PyQtVersion": 6,

    "database":{
        "host":"localhost",
        "name":"ice_store",
        "user":"root",
        "password":"toor",
        "DefaultCursor": "dict"
    },

    "Exe": {
        "Executable": "index.py",
        "OutputFolder": "exe",
        "AddData": [
            "assets",
            "globals",
            "inc/db",
            "modules",
            "Resorces_rc.py",
            "iceStore.ico"
        ]
    },

    "SetupWizard": {
        "MyAppExeName":"exe/IceStore/IceStore.exe",
        "runAsAdministrator":false,
        "OutputDir": "build",
        "OutputBaseFilename": "IceStore",
        "Password": "",
        "files": [
            "exe/IceStore"
        ]
    },

    "Paths": {
        "_comment_": "changeable [Modules,.uiUserInterface ,migrations] ",
        "migrations": "database\\migrations",
         ".uiUserInterface": "ui",
        "Modules": "modules",
        "UiFile": "{UiPath}/{ModuleName}/{ModuleName}.ui",
        ".pyUserInterface": "{ModulesPath}/{ModuleName}/ui/{ModuleName}.py",
        "Models": "{ModulesPath}.{ModuleName}.models.{ModuleName}Model",
        "SubModels": "{ModulesPath}.{ModuleName}.models.{ClassName}Model",
        "MainControllers": "{ModulesPath}.{ModuleName}.{ModuleName}controller.py",
        "Controllers": "{ModulesPath}.{ModuleName}.controllers.{ModuleName}controller.py"

    }
}